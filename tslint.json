{
  "defaultSeverity": "warn",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": false,
  "rules": {
    "trailing-comma": [ false ],
    "no-console": false,
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import |^export {(.*?)}"
      }
    ]
  },
  "rulesDirectory": []
}